{
    "description": "An extension that lets you take to view profiles in one click.",
    "manifest_version": 2,
    "name": "dev-profile-viewer",
    "version": "0.1",
    "version_name": "pre-release",
    "developer": {
        "name": "Gabriel Arazas",
        "url": "https://foo-dogsquared.github.io"
    },
    "homepage_url": "https://github.com/foo-dogsquared/dev-profile-viewer",
    "icons": {
        "48": "icons/dev-profile-viewer-48.png",
        "96": "icons/dev-profile-viewer-96.png"
    },
    "permissions": [
        "*://github.com/*",
        "*://*.github.io/*",
        "*://*.mozilla.org/*",
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/dev-profile-viewer-96.png",
        "default_title": "dev-profile-viewer",
        "default_popup": "/popup/show_url.html",
        "browser_style": true
    }
}
